{
  "manifest_version":2,
  "name":"ThesisExtension",
  "description":"get Chrome History /Login User with Fb/ Get Fb user Likes/ Store everything in Db",
  "version":"0.8",
  "background": {
    "scripts": ["js/trackTime.js", "js/background.js"]
  },
  "browser_action": {
   "default_title": "History",
   "default_popup": "/html/popup.html",
   "default_icon": "/images/history2.png"
 },
  "icons":{
               "16":"/images/history.png",
               "48": "/images/history.png",
              "128": "/images/history.png"
},
  "permissions": [
          "history",
          "tabs",
          "alarms",
          "idle",
          "activeTab",
          "https://floating-depths-67676.herokuapp.com/*",
          "storage"
   ]
}
